  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Program with ts-node",
      "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\back-end\\src\\app.ts", // 指向项目入口文件
      "cwd": "${workspaceFolder}\\back-end", // 设置工作目录
      "env": {
        "NODE_ENV": "development"
      }
    }
        // {
    //   "type": "node",
